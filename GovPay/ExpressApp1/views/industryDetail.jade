extends layout
block content
    div(class='clearfix content')
        div(class="center-column")
            div(class="detail_pane pane_left")
                h2 Wyoming Vendors
                div(class="total_spent")
                    script.
                        document.write(nformatter(#{inStateTotal}));
                ul(class="vendor_list")
                    each val in inStateCompanies
                        li
                            span(class="vendor") #{val.companyName}
                            span(class="spending")
                                script.
                                    document.write(nformatter(#{val.summaryAmount})); 
            div(class="detail_pane pane_right")
                h2 Out Of State Vendors
                div(class="total_spent")
                    script.
                        document.write(nformatter(#{outOfStateTotal}));
                ul(class="vendor_list")
                    each val in outOfStateCompanies
                        li
                            span(class="vendor") #{val.companyName}
                            span(class="spending")
                                script.
                                    document.write(nformatter(#{val.summaryAmount}));   